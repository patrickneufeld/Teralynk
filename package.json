{
  "name": "teralynk",
  "version": "1.0.0",
  "description": "Teralynk project",
  "main": "backend/server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev-frontend\" \"npm run dev-backend\"",
    "dev-frontend": "vite --config frontend/vite.config.js",
    "build-frontend": "vite build --config frontend/vite.config.js",
    "preview-frontend": "vite preview --config frontend/vite.config.js",
    "dev-backend": "nodemon backend/server.js",
    "start-backend": "node backend/server.js",
    "lint-backend": "eslint backend --ext .js --fix",
    "start": "node backend/server.js",
    "setup-database": "node backend/setupDatabase.js",
    "test": "cross-env NODE_ENV=test jest",
    "deploy:dev": "echo 'Deploying to DEV...' && npm run build-frontend",
    "deploy:uat": "echo 'Deploying to UAT...' && npm run build-frontend",
    "deploy:prod": "echo 'Deploying to PROD...' && npm run build-frontend",
    "clean": "rimraf node_modules package-lock.json",
    "reset": "npm run clean && npm install"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.530.0",
    "@aws-sdk/client-s3": "^3.530.0",
    "@aws-sdk/client-secrets-manager": "^3.530.0",
    "@aws-sdk/s3-request-presigner": "^3.530.0",
    "@testing-library/user-event": "^14.6.10",
    "aws-sdk": "^2.1610.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.3",
    "express-rate-limit": "^7.0.0",
    "express-validator": "^7.0.1",
    "file-type": "^18.2.0",
    "formidable": "^3.5.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "mongoose": "^8.4.3",
    "morgan": "^1.10.0",
    "natural": "^8.1.0",
    "react-google-recaptcha": "^3.1.0",
    "react-router-dom": "^6.22.2",
    "redis": "^4.6.7",
    "shortid": "^2.2.16",
    "trufflehog": "^3.45.2",
    "util": "^0.12.5",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.4",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.2",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-security": "^2.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "supertest": "^7.2.4",
    "typescript": "^5.4.3"
  },
  "author": "",
  "license": "ISC"
}
