    "@aws-sdk/s3-request-presigner": "3.470.0",
    "@fast-csv/format": "4.3.5",
    "amqplib": "0.10.3",
    "axios": "^1.9.0",
    "bcrypt": "5.1.1",
    "body-parser": "^1.20.2",
    "compression": "1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dompurify": "^3.2.6",
    "dotenv": "^16.3.1",
    "dropbox": "10.34.0",
