  "devDependencies": {
    "@eslint/js": "8.55.0",
    "eslint": "8.55.0",
    "eslint-plugin-react": "7.33.2",
    "globals": "13.23.0",
    "jest": "29.7.0",
    "nodemon": "3.0.2",
    "prettier": "3.1.0",
    "supertest": "6.3.3"
